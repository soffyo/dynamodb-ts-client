{
  "name": "dynamodb-ts-client",
  "version": "1.0.8",
  "description": "DynamoDB Client with Typescript support",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.d.ts",
    "generator"
  ],
  "scripts": {
    "build": "tsc",
    "git": "git add . && git commit -m",
    "postgit": "git push",
    "prepare": "npm run build",
    "version": "git add .",
    "postversion": "git push",
    "prepublishOnly": "npm run test",
    "dynamodb": "java -Djava.library.path=./dynamodb-local/DynamoDBLocal_lib -jar ./dynamodb-local/DynamoDBLocal.jar",
    "test": "concurrently --kill-others -s \"first\" -n \"DynamoDB,Jest\" -c \"bgBlue.bold.white,bgYellow.bold.white\" \"npm:dynamodb\" \"jest\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soffyo/dynamodb-ts-client.git"
  },
  "keywords": [
    "aws",
    "dynamodb",
    "client",
    "typescript"
  ],
  "author": "soffyo <robi.slick@gmail.com> (https://soffyo.github.io)",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/soffyo/dynamodb-ts-client/issues"
  },
  "homepage": "https://github.com/soffyo/dynamodb-ts-client#readme",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.29",
    "concurrently": "^7.1.0",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.79.0",
    "@aws-sdk/lib-dynamodb": "^3.79.0"
  }
}
